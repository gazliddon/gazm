SGS     FCB 6,$44,$9,$D0                                
        FCB 4,$48,7,$D0                                 
        FCB 3,$4A,$6,$D0                                
        FCB 2,$4C,$5,$D0                                
        FCB $82                         ; REPEAT TWICE  
        FCB 1,$4E,$4,$D0                                
        FCB $8D                         ; REPEAT 13     
        FCB $50,$3,$D0                                  
        FCB $82                         ; REPEAT TWICE  
        FCB $50,4,$CE                                   
        FCB $50,5,$CC                                   
        FCB $50,6,$CA                                   
        FCB $50,7,$C8                                   
        FCB $50,9,$C4                                   

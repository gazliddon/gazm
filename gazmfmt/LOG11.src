; STARGATE MESSAGE TABLE                                                
; LETTER,COLOR,DELAY,FLAVOR,X16                                                
SGMTAB  equ *                                           
; LETTER S                                                
        FDB SGS                                         
        FCB $11,$F0                                     
        FDB $0000                                       
; LETTER T                                                
        FDB SGT                                         
        FCB $22,$F0                                     
        FDB $1000                                       
; LETTER A                                                
        FDB SGA                                         
        FCB $33,$F0                                     
        FDB $1F00                                       
; LETTER R                                                
        FDB SGR                                         
        FCB $44,$F0                                     
        FDB $2F00                                       
; LETTER G                                                
        FDB SGG                                         
        FCB $55,$F0                                     
        FDB $3F00                                       
; LETTER A                                                
        FDB SGA                                         
        FCB $66,$F0                                     
        FDB $4F00                                       
; LETTER T                                                
        FDB SGT                                         
        FCB $77,$F0                                     
        FDB $5F00                                       
; LETTER E                                                
        FDB SGE                                         
        FCB $88,$F0                                     
        FDB $6F00                                       
        FDB 0                           ; END           
